            position:absolute;
            align-items: center;
        }
        .qu div{
            font-size: 16px;
            font-family: "Archivo Black", sans-serif;
            color: #354147;
        }
        .msg{
            height: 100%;
            width: 100%;
            position:absolute;
            z-index: 1;
            top:0;
            transition: display 1s ease;
            display: flex;
            flex-direction: column;
            background-color: rgba(0,0,0,.85);
        }
        .msg textarea{
            resize: none;
            width: 95%;
            border: none;
            background:none;
            color: #39FF14;
            letter-spacing: 3px;
            font-size: 25px;
            transition:all .2s ease;
            font-family: "Lobster", sans-serif;
            position: relative;
        }
        .msg textarea::placeholder{
            width: 100%;
            transform: translateY(-50%);
            display: flex;
            line-height: 290px;
            top: 50%;
            font-family: "Arial";
            font-size: 30px;
            text-transform: uppercase;
            font-weight: 200;
            color: #04d9ff;
            text-align: center;
            letter-spacing: 4px;
            position: absolute;
        }
        .msg textarea:focus{
            outline: none;
        }
        .msg .des{
            width: calc(100% - 20px );
            top: 60px;
            display: flex;
            align-items: center;
            justify-content: center;
            padding: 15px 0;
            left: 10px;
            background-color: #fff2;
            position:relative;
        }
        .msg .tit{
            position: relative;
            background-color: #fff2;
            width: calc(100% - 20px);
            left: 10px;
            height: 70px;
            top: 40px;
            display: flex;
            align-items: center;
            justify-content: center;
        }
        .msg .tit .title{
            height: 50%;
            width: 95%;
            background: none;
            border: none;
            font-size: 25px;
            color: #39FF14;
            text-transform: uppercase;
            text-decoration: underline;
            font-family: "Arial", sans-serif;
        }
        .msg .title::placeholder{
            width: 100%;
            transform: translateY(-50%);
            top: 50%;
            font-family: "Arial";
            font-size: 30px;
            text-transform: uppercase;
            font-weight: 500;
            text-decoration: none;
            color: #04d9ff;
            text-align: center;
            letter-spacing: 3px;
            position: absolute;
        }
        .msg .title:focus{
            outline: none;
        }
        .msg .button{
            background: none;
            color: #f72119;
            border: white 2px solid;
            border-radius: 20px;
            cursor: pointer;
            font-weight: 600;
            height: 50px;
            width: 80%;
            text-transform: uppercase;
            transition: background .5s ease, color .5s ease;
            letter-spacing: 3px;
            left: 10%;
            font-size: 20px;
            font-family: "Arial", sans-serif;
            bottom: 30px;
            position:absolute;
        }
        .msg .button:hover{
            border: none;
            background: #f72119;
            color: white;
        }
        .cross{
            display: flex;
            justify-content: center;
            align-items: center;
            height: 40px;
            cursor: pointer;
            width: 40px;
            position:absolute;
            z-index: 99;
            top: 0;
            right: 0;
        }
        .cross .up{
            height: 3px;
            width: 70%;
            transform: rotateZ(45deg);
            background-color: white;
            position:absolute;
        }
        .cross .down{
            height: 3px;
            width: 70%;
            transform: rotateZ(-45deg);
            background-color: white;
            position:absolute;
        }
        .donemsg{
            position: absolute;
            height: 40px;
            width: calc(40% - 10px);
            left: 5px;
            top: 5px;
            color: white;
            box-sizing: border-box;
            line-height: 40px;
            padding-left: 10px;
            z-index: 99;
            font-size: 20px;
            font-family: "Arial";
            border-radius: 5px;
            border: darkblue 2px solid;
            background-color: teal;
        }
        .message{
            position:absolute;
            height: 100vh;
            width: 100vw;
            background-color: #0009;
            z-index: 100;
            display: flex;
            align-items: center;
            justify-content: center;
        }
        .message .box{
            height: 50rem;
            width: 80rem;
            position:relative;
            background-color: #E0E722;
            border-radius: 10rem;
            border: 5rem solid tomato;
        }
        .message .text{
            height: 60%;
            width: 100%;
            position: relative;
            background-color: tomato;
            color: #e0e722;
            display: flex;
            flex-direction: column;
        }
        .message .text .titlees::after, .message .text .descees::after{
            content: "Title";
            position:absolute;
            font-size: 1.4rem;
            height: 4rem;
            font-weight: 600;
            text-transform: uppercase;
            color: #e0e722;
            line-height: 100%;
            font-family: "Arial";
            left: 0;
            top: 1.5rem;
        }
        .message .text .descees::after{
            content: "Description";
            right: 0;
        }
        .message .text .titlees{
            height: 40%;
            font-weight: 600;
            border-bottom: 4px solid #e0e722;
            font-size: 5rem;
            text-transform: uppercase;
            font-family: "Arial", sans-serif;
            line-height: 12rem;
            box-sizing: border-box;
            padding-left: 5rem;
            width: 100%;
        }
        .message .text .descees{
            box-sizing: border-box;
            height: 56%;
            text-align: right;
            padding-right: 3rem;
            position:relative;
            font-size: 2.3rem;
            padding-top: 4rem;
            font-family: "Lobster", sans-serif;
            width: 100%;
        }
        .message .btns{
            height: 40%;
            width: 100%;
            display: flex;
            justify-content: space-evenly;
            align-items: center;
            position: relative;
        }
        .message .btns form input{
            background: none;
            box-shadow: 
                inset 2rem 2rem 0 tomato,
                1rem 1rem 0 blue;
            height: 10rem;
            border: none;
            width: 25rem;
            font-family: "Black Ops One", sans-serif;
            color: tomato;
            font-size: 3rem;
            transition: all .3s ease;
            box-sizing: border-box;
            cursor: pointer;
            border-radius: 5rem;
        }
        .message .btns form input:hover{
            box-shadow: 
                inset 1rem 1rem 0 tomato,
                2rem 2rem 0 blue;
            padding-left:2rem;
            padding-top: 2rem;
            font-size: 4rem;
            color: blue;
        }

    </style>

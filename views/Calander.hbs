        .dot{
            height: 20px;
            width: 20px;
            border-top-right-radius: 70%;
            border-top-left-radius: 50%;
            border-bottom-left-radius: 70%;
            border-bottom-right-radius: 50%;
            position:absolute;
            left: 50%;
            z-index: 999;
            background-color: black;
            top: 50%;
            transform: translate(-10px, -10px)
        }
        .calendersect{
            left: 43%;
            top: 5vh;
            user-select: none;
            position: relative;
            height: 90vh;
            background-color: #354147;
            width: 55%;
        }
        .calendersect .top{
            width: 100%;
            height: 20vh;
            display: flex;
            justify-content: space-between;
            align-items: center;
            font-family: "Black Ops One", sans-serif;
            font-size: 40px;
        }
        .calendersect .top .Month-name .Year{
            font-size: 20px;
        }
        .calendersect .top .Month-name{
            color: #FFA0A0;
            text-transform: uppercase;
            line-height: 25px;
            text-align: center;
            font-size: 60px;
        }
        .calendersect .top .left, .calendersect .top .right{
            color: #FFE3E3;
            cursor: pointer;
        }
        .digitalclk{
            height: 50px;
            width: 400px;
            text-align: center;
            font-weight: 100;
            color: #0073e6;
            left: 100px;
            font-family: "Archivo Black", sans-serif;
            font-size: 50px;
            bottom: 100px;
            position: absolute;
        }

        .calendersect .days{
            box-sizing: border-box;
            display: flex;
            background-color: #628395;
            width: 100%;
            font-size: 20px;
            font-family: "Black Ops One", sans-serif;
            height: 10vh;
            color: #FFA0A0;
            text-align: center;
            align-items: center;
        }
        .calendersect .days div{
            width: calc(100% / 7);
        }

        .calendersect .dates div span:hover{
            border-color: greenyellow;
            color: #0073e6;
        }
        .calendersect .dates{
            width: 100%;
            box-sizing: border-box;
            height: 60vh;
            display: flex;
            align-items: center;
            flex-wrap: wrap;
        }
        .calendersect .dates .prev, .calendersect .dates .next{
            opacity: .4;
        }
        .calendersect .dates .today{
            height: 60px;
            display: inline-block;
            width: 60px;
            border-radius: 10px;
            color: #354147;
            display: flex;
            justify-content: center;
            align-items: center;
            position:absolute;
            background-color: greenyellow;
        }
        .calendersect .dates div span{
            height: 60px;
            display: inline-block;
            width: 60px;
            cursor: pointer;
            transition: .2s ease;
            border: 3px solid #354147;
            box-sizing: border-box;
            border-radius: 10px;
            display: flex;
            justify-content: center;
            align-items: center;
            position:absolute;
        }
        .calendersect .dates div{
            position:relative;
            width: calc(100% / 7);
            height: calc(100% / 7);
            font-size: 20px;
            font-family: "Black Ops One", sans-serif;
            display: flex;
            justify-content: center;
            align-items: center;
            color: #FFE3E3;
        }

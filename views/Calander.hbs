        <div class="top">
            <span class="left"><</span>
            <span class="Month-name"></span>
            <span class="right">></span>
        </div>

        <div class="days">
            <div>Sun</div>
            <div>Mon</div>
            <div>Tue</div>
            <div>Wed</div>
            <div>Thr</div>
            <div>Fri</div>
            <div>Sat</div>
        </div>

        <div class="dates">
            <!-- div*31{$} -->
        </div>

        <div class="msg">
            <div class="cross mssg">
                <div class="up"></div>
                <div class="down"></div>
            </div>
            <form action="/done" method="post">
                <div class="tit"><input type="text" placeholder="Title" class="title" name="title"></div>
                <div class="des"><textarea class="description" rows="10" cols="30" placeholder="Description" name="desc"></textarea></div>
                <input type="text" name="datess" class="dateen" style="display:none">
                <input type="submit" class="button" value="Create Reminder">
            </form>
        </div>
    </div>
    <script>
        let datee = new Date();
        const mon = [
            "January",
            "February",
            "March",
            "April",
            "May",
            "June",
            "July",
            "August",
            "September",
            "October",
            "November",
            "December"
        ];
        function datess(){
            var datesOfMonth = "";
            $(".dates").empty();
            $(".Month-name").empty();
            $(".Month-name").append(`${mon[datee.getMonth()]} <br> <span class="Year">${datee.getFullYear()}</span></div>`);
            var prre = new Date(datee.getFullYear(), datee.getMonth(), 1).getDay();
            var pree = new Date(datee.getFullYear(), datee.getMonth(), 0).getDate();
            var thiss = new Date(datee.getFullYear(), datee.getMonth()+1, 0).getDate();
            var thissday = new Date(datee.getFullYear(), datee.getMonth()+1, 0).getDay();
            for(let i=prre; i>0; i--){
                let year, month;
                if(datee.getMonth() == 0){
                    year = datee.getFullYear() -1;
                    month = 11;
                }else{
                    year = datee.getFullYear();
                    month = datee.getMonth()-1;
                }
                datesOfMonth += `<div class="date prev"><span class="${pree-i+1} ${month} ${year}">${pree - i + 1}</span></div>`;
            }
            for(let i=1; i<=thiss; i++){
                if(i == datee.getDate() && datee.getMonth() == new Date().getMonth() && datee.getFullYear() == new Date().getFullYear()){
                    datesOfMonth += `<div class="date"><span class="today ${i} ${datee.getMonth()} ${datee.getFullYear()}">${i}</span></div>`;
                    continue;
                }
                datesOfMonth += `<div class="date"><span class="${i} ${datee.getMonth()} ${datee.getFullYear()}">${i}</span></div>`;
            }
            for(let i=1; i<7-thissday; i++){
                let year, month;
                if(datee.getMonth() == 11){
                    year = datee.getFullYear() +1;
                    month = 0;
                }else{
                    year = datee.getFullYear();
                    month = datee.getMonth()+1;
                }
                datesOfMonth += `<div class="date next"><span class="${i} ${month} ${year}">${i}</span></div>`;
            }
            $(".dates").append(datesOfMonth);
            
            $(".date span").click(function(){
                let bbs = $(this).attr("class").split(" ");
                let td = new Date(); 
                if((bbs[bbs.length-1] >= td.getFullYear() && bbs[bbs.length-2] > td.getMonth()) || (bbs[bbs.length-1] == td.getFullYear() && bbs[bbs.length-2] == td.getMonth() && bbs[bbs.length-3] > td.getDate())){
                    $(".msg").css("display", "block");
                    $(".msg").css("pointer-events","all");
                    $(".dateen").val(`${bbs[bbs.length-3]}/${bbs[bbs.length-2]}/${bbs[bbs.length-1]}`);
                }
            });
        }
        datess();
        if($(".titlees").is(":empty") && $(".descees").is(":empty")){
                $(".message").css("display", "none");
                $(".message").css("pointer-events", "none");
            }
            if($(".mmssg").is(":empty")){
                $(".donemsg").css("display","none");
            }
            $(".msg").css("display", "none");
            $(".msg").css("pointer-events", "none");
        function sssec(){
                let date = new Date();
                var sec = date.getSeconds();
                var hour = date.getHours();
                var ssec = (sec * 360) / 60 + "deg";
                var min = date.getMinutes();
                var smin = (min * 360) / 60 + "deg";
                var shour = (((hour % 12) * 360) / 12) + "deg";
                $(".hour").css("transform", `rotateZ(${shour})`);
                $(".minute").css("transform", `rotateZ(${smin})`);
                $(".second").css("transform", `rotateZ(${ssec})`);
                $(".digitalclk").empty();
                var am = "AM";
                if(min < 10) min = "0" + min;
                if(hour > 12) am = "PM";
                if(hour > 12) hour = hour%12;
                if(hour < 10) hour = "0" + hour;
                if(sec < 10) sec = sec = "0" + sec;
                $(".meri").empty();
                $(".meri").append(am);
                $(".digitalclk").append(`${hour} : ${min} : ${sec}`);
            }
            sssec();
            setInterval(sssec, 200);
            $(".mssg").click(function(){
                $(".msg").css("display","none");
            });
        $("window").ready(function(){
            $(".description").focus(function(){
                $(".des").css("outline","3px solid #4D4DFF")
            });
            $(".description").blur(function(){
                $(".des").css("outline","none")
            });
            $(".title").focus(function(){
                $(".tit").css("outline","3px solid #4D4DFF")
            });
            $(".title").blur(function(){
                $(".tit").css("outline","none")
            });
            $(".donms").click(function(){
                $(".donemsg").css("display", "none");
            });
            $(".right").click(function(){
                if(datee.getMonth() < 11){
                    datee.setMonth(datee.getMonth()+1);
                    datess();
                }else{
                    datee.setFullYear(datee.getFullYear()+1);
                    datee.setMonth(0);
                    datess();
                }
            });
            $(".left").click(function(){
                if(datee.getMonth() > 0){
                    datee.setMonth(datee.getMonth()-1);
                    datess();
                }else{
                    datee.setFullYear(datee.getFullYear()-1);
                    datee.setMonth(11);
                    datess();
                }
            });
            
        });

    </script>
</body>
</html>
